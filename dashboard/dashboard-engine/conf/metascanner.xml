<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<access-check-config-file-name>/etc/dashboard/conf/access-check-config.xml</access-check-config-file-name>
	<app-root-dir>/var/dashboard</app-root-dir>
	<meta-scanner>
		<log-home>/var/dashboard/log/meta-scanner</log-home>
		<log-config-file>/etc/dashboard/conf/meta-scanner-logback.xml</log-config-file>
		<meta-uptime-interval>3600000</meta-uptime-interval>
		<executor>
			<core-size>4</core-size>
			<max-size>4</max-size>
			<queue-size>1</queue-size>
		</executor>
		<deploy>
			<version-file>/etc/dashboard/conf/metascanner-version.info</version-file>
		</deploy>
	</meta-scanner>
</configuration>