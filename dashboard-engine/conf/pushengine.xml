<?xml version="1.0" encoding="UTF-8"?>   
<configuration> 
	<access-check-config-file-name>/etc/dashboard/conf/access-check-config.xml</access-check-config-file-name>
	<app-root-dir>/var/dashboard</app-root-dir>
	<engine>
		<server>
			<port>8010</port>
			<log-home>/var/dashboard/log/pushengine</log-home>
			<log-config-file>/etc/dashboard/conf/pushengine-logback.xml</log-config-file>
			<meta-uptime-interval>3600000</meta-uptime-interval>
			<request>
				<max-content-length>33554432</max-content-length>
				<request-executor>
					<core-size>48</core-size>
					<max-size>96</max-size>
					<queue-size>8192</queue-size>
				</request-executor>
				<longtime-request-executor>
					<core-size>8</core-size>
					<max-size>16</max-size>
					<queue-size>1024</queue-size>
				</longtime-request-executor>
				<access-rules>
					<access-rule>
						<uri>*</uri>
						<allowed-ip>*</allowed-ip>
					</access-rule>
				</access-rules>
			</request>
		</server>
		<mail>
			<tos>jiangw@ctrip.com,zlsong@ctrip.com,wzwang@ctrip.com</tos>
			<ccs>jiangw@ctrip.com</ccs>
			<smtp-host>appmail.sh.ctriptravel.com</smtp-host>
			<smtp-account>Rdkjda@Ctrip.com</smtp-account>
		</mail>
		<deploy>
			<version-file>/etc/dashboard/conf/pushengine-version.info</version-file>
		</deploy>
	</engine>
</configuration>